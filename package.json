{
  "name": "nova-build",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --webpack",
    "dev:clean": "rm -rf .next && next dev",
    "dev:reset": "rm -rf .next node_modules/.cache && next dev",
    "dev:fresh": "rm -rf .next node_modules/.cache node_modules && npm install && next dev",
    "dev:auto-clean": "node scripts/dev-cache-cleaner.js & next dev",
    "dev:smart": "npm run clean && next dev",
    "dev:enhanced": "node scripts/enhanced-cache-manager.js dev",
