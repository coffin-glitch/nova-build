    "health:check": "node scripts/enhanced-cache-manager.js health",
    "prebuild": "if [ \"$VERCEL\" != \"1\" ]; then rm -rf .next; fi",
    "build": "next build --webpack && mkdir -p .next && touch .next/lock",
    "start": "next start",
    "lint": "eslint",
    "clean": "rm -rf .next node_modules/.cache",
    "clean:all": "rm -rf .next node_modules/.cache node_modules",
    "fresh": "npm run clean:all && npm install",
    "cache:check": "node scripts/cache-monitor.js",
    "ingest:eax": "tsx scripts/eax-ingest.ts",
